<% if @current_user%>
  <h1>Places</h1>

  <ul>
    <% for place in @places %>
      <li><a href="/places/<%= place["id"] %>" class="btn btn-primary"><%= place["name"] %></a></li>
    <% end %>
  </ul>

  <p>
    <a href="/places/new" class="btn btn-success">New place</a>
  </p>
<% else %>
<p>
 You must be logged in
</p>
<% end %>